{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "cd .. && cd ..&& husky install infra-template/MultiTenantHub/.husky"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "@turbo/gen": "^1.10.2",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "turbo": "^1.10.2"
  },
  "lint-staged": {
    "apps/**/*.{js,ts,jsx,tsx}": [
      "pnpx eslint --fix "
    ],
    "packages/**/*.{js,ts,jsx,tsx}": [
      "pnpx eslint --fix"
    ],
    "*.json": [
      "pnpx prettier --write"
    ]
  },
    "packageManager": "pnpm@8.6.1",
  "name": "multi-tenant-hub"
}
